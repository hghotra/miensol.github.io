    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-remark-embed-snippet": "^4.3.0",
    "gatsby-plugin-page-progress": "^2.0.16",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "invariant": "^2.2.4",
