    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "lost": "8.3.1",
    "postcss-pxtorem": "4.0.1",
    "react-test-renderer": "16.12.0",
    "rimraf": "3.0.2",
