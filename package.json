    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.34",
    "@types/react-test-renderer": "^16.9.2",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/react-helmet": "^5.0.15",
    "@types/classnames": "^2.2.10",
    "autoprefixer": "9.7.4",
